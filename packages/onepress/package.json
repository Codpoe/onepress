{
  "name": "onepress",
  "version": "0.1.9",
  "description": "Static site generator based on Vite & vite-plugin-react-pages",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Codpoe/onepress.git"
  },
  "author": "codpoe <codpoe.me@gmail.com>",
  "license": "MIT",
  "bin": {
    "onepress": "bin/onepress.js"
  },
  "files": [
    "bin",
    "dist",
    "theme.js",
    "theme.d.ts"
  ],
  "scripts": {
    "dev": "run-p --print-label dev:*",
    "dev:node": "tsc -w -p src/node",
    "dev:client": "tsc -w -p src/client",
    "build": "rimraf dist && run-p --print-label build:* && node scripts/genEntry.js",
    "build:node": "tsc -p src/node",
    "build:client": "tsc -p src/client"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "cac": "^6.7.3",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "debug": "^4.3.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "gray-matter": "^4.0.3",
    "jiti": "^1.10.1",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "polka": "^0.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "remark-slug": "^6.0.0",
    "sirv": "^1.0.12",
    "vite": "^2.3.7",
    "vite-pages-theme-press": "workspace:^",
    "vite-plugin-mdx": "^3.5.6",
    "vite-plugin-react-pages": "^3.0.14"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/polka": "^0.5.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "rollup": "^2.38.5"
  }
}
