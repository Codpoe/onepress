{
  "name": "onepress",
  "version": "0.1.23",
  "description": "Static site generator based on vite",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": "./dist/node/index.js",
    "./client": "./dist/client/index.js",
    "./theme": "./dist/theme/index.ts",
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Codpoe/onepress.git"
  },
  "author": "codpoe <codpoe.me@gmail.com>",
  "license": "MIT",
  "bin": {
    "onepress": "bin/onepress.js"
  },
  "files": [
    "bin",
    "dist",
    "client.d.ts",
    "theme.d.ts",
    "shim.d.ts"
  ],
  "scripts": {
    "dev": "run-p -l dev:*",
    "dev:node": "tsc -w -p src/node",
    "dev:client": "tsc -w -p src/client",
    "dev:theme": "node scripts/copyTheme.js -w",
    "build": "rimraf dist && run-p -l build:*",
    "build:node": "tsc -p src/node",
    "build:client": "tsc -p src/client",
    "build:theme": "node scripts/copyTheme.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "@docsearch/css": "^3.0.0-alpha.42",
    "@docsearch/react": "^3.0.0-alpha.42",
    "@iconify/json": "^1.1.443",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@vitejs/plugin-react": "^1.1.3",
    "autoprefixer": "^10.4.0",
    "cac": "^6.7.12",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "compression": "^1.7.4",
    "debug": "^4.3.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "gray-matter": "^4.0.3",
    "jest-docblock": "^27.0.6",
    "jiti": "^1.12.9",
    "less": "^4.1.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "ora": "^5.4.1",
    "polka": "^0.5.2",
    "postcss": "^8.4.5",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.1.1",
    "remark-slug": "^6.0.0",
    "sirv": "^1.0.19",
    "tailwindcss": "^3.0.5",
    "unplugin-icons": "^0.12.23",
    "valtio": "^1.2.7",
    "vite": "^2.7.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/mdx-js__react": "^1.5.3",
    "@types/polka": "^0.5.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "fast-glob": "^3.2.7",
    "listr2": "^3.13.5",
    "rollup": "^2.38.5",
    "semver": "^7.3.5"
  }
}
